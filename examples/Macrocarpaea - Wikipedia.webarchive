	obj.plain = obj.plain.replace(/&amp;ndash;/g, 'â€“');
	obj.plain = obj.plain.replace(/&amp;minus;/g, '\u2212');

	// remove spaces around em dashes
	var regExp = new RegExp('([' + wikEd.letters + '_0-9\'"â€\\]})])( |&amp;nbsp;)*â€”( |&amp;nbsp;)*([' + wikEd.letters + '_0-9\'"â€œ\\[{(])', 'g');
	obj.plain = obj.plain.replace(regExp, '$1â€”$4');

	// convert -- to em dashes
	var regExp = new RegExp('([' + wikEd.letters + '_0-9\'"â€\\]})])( |&amp;nbsp;)*--( |&amp;nbsp;)*([' + wikEd.letters + '_0-9\'"â€œ\\[{(])', 'g');
	obj.plain = obj.plain.replace(regExp, '$1â€”$4');

