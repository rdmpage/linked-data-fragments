			}
			else if ('|tr|'.indexOf(tagCheck) >= 0) {
				if ((common + '|bgcolor|' + tablealign).indexOf(attribCheck) >= 0) { valid = true; }
			}
			else if ('|td|th|'.indexOf(tagCheck) >= 0) {
