//
// wikEd.FixUnits: unit formatter
//

wikEd.FixUnits = function (obj) {

	wikEd.FixBasic(obj);

	// convert into actual characters
	obj.plain = obj.plain.replace(/&amp;deg;|&amp;#00b0;/g, 'Â°');
	obj.plain = obj.plain.replace(/&amp;#00b5;|&amp;mu;|&amp;micro;/g, 'Âµ');
