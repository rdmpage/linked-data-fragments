	obj.plain = obj.plain.replace(/(^|[ \(\|])(\d\d(\d\d)?)(\u2212|-|â€“)(\d\d)(\u2212|-|â€“)(\d\d(\d\d)?)([ \)\}\|,.;â€”]|$)/gm, '$1$2â€“$5â€“$7$9');

