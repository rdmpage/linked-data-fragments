	var regExp = new RegExp('([' + wikEd.letters + '_0-9\'"â€\\]>] ) *(\\u2212|â€“)(\\d)', 'g');
	obj.plain = obj.plain.replace(regExp, '$1\u2212$3');

	// convert minus or en dashes to dashes with spaces
	var regExp = new RegExp('([' + wikEd.letters + '_0-9\'"â€\\]}])( |&amp;nbsp;)*(\\u2212|â€“)( |&amp;nbsp;)*([' + wikEd.letters + '_0-9\'"â€œ\\[{])', 'g');
